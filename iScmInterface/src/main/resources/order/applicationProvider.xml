<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">

    <!-- Application name -->
    <dubbo:application name="OrderServer" />
    
    <dubbo:provider timeout="5000" cluster="failfast"/>
    <!-- registry address, used for service to register itself -->
    <dubbo:registry client="zkclient" address="${zookeeper.address}" />
    <!-- 用dubbo协议在20992端口暴露服务 -->
    <dubbo:protocol name="dubbo" port="20993" accepts="1000"/>


    <!-- Service interface   Concurrent Control 生产者服务  -->
    <dubbo:service  interface="com.zzhw.gtms.order.service.IIonicService"  ref="ionicService"/>

</beans>